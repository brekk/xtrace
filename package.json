{
  "name": "xtrace",
  "version": "0.0.10",
  "description": "trace compositions",
  "license": "ISC",
  "main": "xtrace.js",
  "reveal": true,
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "reporter": [
      "lcov",
      "json",
      "html",
      "text",
      "text-summary"
    ],
    "exclude": [
      "docs",
      "coverage",
      "config",
      "node_modules",
      "src/*.spec.js",
      "src/*.fixture.js",
      "src/core/index.js",
      "src/core/trace.js",
      "src/core/xtrace.js",
      "src/index.js",
      "xtrace.js",
      "xtrace.es.js"
    ],
    "include": [
      "src/*.js",
      "src/**/*.js",
      "!src/*.spec.js"
    ],
    "all": true,
    "check-coverage": true,
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "care": "nps care",
    "precommit": "nps care"
  },
  "dependencies": {
    "katsu-curry": "^0.1.1",
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "documentation": "^4.0.0-rc.1",
    "eslint": "4.2.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-better": "^0.1.5",
    "eslint-plugin-eslint-comments": "^1.0.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "3.0.1",
    "execa": "^0.8.0",
    "f-utility": "^3.2.0",
    "husky": "0.14.3",
    "istanbul": "0.4.x",
    "nyc": "^11.0.3",
    "rollup": "^0.43.0",
    "rollup-plugin-babili": "^3.1.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-progress": "^0.4.0",
    "strip-ansi": "^4.0.0"
  },
  "repository": "https://github.com/brekk/xtrace",
  "author": "brekk"
}
